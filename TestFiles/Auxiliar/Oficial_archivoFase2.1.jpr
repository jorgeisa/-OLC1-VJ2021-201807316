}

func BubbleSort(int[] miVector){
    for (var i = 0; i < length(miVector); i++)
    {
        for (var j = 0; j < length(miVector) - i - 1; j++)
        {
            if (miVector[j] > miVector[j + 1]) {
                var temp;
                temp = miVector[j];
                miVector[j] = miVector[j + 1];
                miVector[j + 1] = temp;
            }
        }
    }
}